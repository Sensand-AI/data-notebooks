{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "notebook_name": {
      "type": "string"
    },
    "save_output": {
      "type": "boolean"
    },
    "output_type": {
      "type": "string"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "geojson": {
          "type": "object"
        },
        "propertyName": {
          "type": "string"
        },
        "colormap": {
          "type": "string"
        },
        "slga_layer": {
          "type": "string"
        },
        "slga_layer_depth": {
          "type": "string"
        }
      },
      "required": ["geojson", "propertyName"]
    }
  },
  "required": ["notebook_name", "parameters", "output_type"]
}
