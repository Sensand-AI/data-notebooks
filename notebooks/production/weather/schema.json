{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "notebook_name": {
      "type": "string"
    },
    "save_output": {
      "type": "boolean"
    },
    "output_type": {
      "type": "string"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "geojson": {
          "type": "object"
        },
        "propertyName": {
          "type": "string"
        },
        "forecast_days": {
          "type": "integer"
        },
        "start_year": {
          "type": "integer"
        },
        "end_year": {
          "type": "integer"
        },
        "historical": {
          "type": "boolean"
        },
        "forecast": {
          "type": "boolean"
        }
      },
      "required": ["geojson", "propertyName", "forecast_days", "historical", "forecast", "start_year", "end_year"]
    }
  },
  "required": ["notebook_name", "parameters", "output_type"]
}
